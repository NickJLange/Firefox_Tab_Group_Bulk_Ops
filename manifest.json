{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/webextensions/tree/main/group-same-site-tabs",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_area": "navbar"
  },
  "commands": {
    "group-selected": {
      "description": "Group Selected Sites"
    },
    "group-all": {
      "description": "Group All Sites"
    },
    "group-selected-uncollapsed": {
      "description": "Group Selected Sites (uncollapsed)"
    },
    "group-all-unclollapsed": {
      "description": "Group All Sites (uncollapsed)"
    },
    "group-selected-base": {
      "description": "Group Selected Sites (Base Domain)"
    },
    "group-all-base": {
      "description": "Group All Sites (Base Domain)"
    },
    "ungroup-all": {
      "description": "Ungroup All Tab Groups"
    }
  },
  "icons": {},
  "description": "Move same site/domain tabs into named collapsed or unclollapsed native tabgroups",
  "manifest_version": 2,
  "name": "Group Same Site Tabs",
  "permissions": [
    "tabs",
    "menus",
    "tabGroups"
  ],
  "version": "1.0.0"
}